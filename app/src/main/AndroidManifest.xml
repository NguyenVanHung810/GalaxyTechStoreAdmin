<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.galaxytechstoreadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GalaxyTechStoreAdmin">
        <activity
            android:name=".OrderDetailsActivity"
            android:label="@string/title_activity_order_details"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar"></activity>
        <activity
            android:name=".UpdateUserInfoActivity"
            android:label="@string/title_activity_update_user_info"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar" />
        <activity
            android:name=".Login_Register_ResetPassword_Activity"
            android:label="@string/title_activity_login__register__reset_password_"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:label="@string/title_activity_splash"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddProductActivity"
            android:label="@string/title_activity_add_product"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.GalaxyTechStoreAdmin.NoActionBar" />
    </application>

</manifest>